<p class=\"typing-text\">Select one for movie night</p>\n    </div>\n    <div class=\"search-container\">\n      <input\n          type=\"text\"\n          v-model=\"searchKeyword\"\n          @keydown=\"handleKeydown\"\n          placeholder=\"ËæìÂÖ•ÁîµÂΩ±Âêç / ÂØºÊºî / ÊºîÂëòÁ≤æÁ°ÆÊêúÁ¥¢Ô∏èüôÇ ÊàñËÄÖ ËæìÂÖ•ÁîµÂΩ±ÊèèËø∞Ê®°Á≥äÊêúÁ¥¢üò∂‚Äçüå´\"\n          class=\"search-box\"\n      />\n      <button @click=\"searchByKeyword\" class=\"search-button\">Á≤æÁ°ÆÊêúÁ¥¢</button>\n      <button @click=\"fuzzySearch\" class=\"fuzzy-search-button\">Ê®°Á≥äÊêúÁ¥¢</button>\n    </div>\n    <div class=\"category-search-container\">\n      <button @click=\"fetchAllMovies\" class=\"view-all-button\">Êü•ÁúãÂÖ®ÈÉ®ÁîµÂΩ±üòé</button>\n      <button @click=\"goToTagsSearch\" class=\"category-search-button\">ÈÄöËøáÊ†áÁ≠æÊêúÁ¥¢üòé</button>\n    </div>\n\n    <!-- ‰∏âÂàóÂ∏ÉÂ±ÄÂÆπÂô® -->\n    <div class=\"three-column-container\">\n      <!-- Â∑¶‰æßÔºöÁÉ≠Â∫¶Ê¶ú -->\n      <div class=\"side-column\">\n        <h3 class=\"column-title\">ÁÉ≠Â∫¶Ê¶ú</h3>\n        <div\n            v-for=\"(movie, index) in topMovies\"\n            :key=\"movie._id\"\n            class=\"side-item\"\n            @click=\"goToMovieDetail(movie._id)\"\n        >\n          <div class=\"side-content\">\n            <span class=\"side-index\">{{ index + 1 }}.</span>\n            <span class=\"side-title\">{{ movie.title }}</span>\n          </div>\n          <span class=\"side-tags\">{{ movie.tags.join(', ') }}</span>\n        </div>\n      </div>\n\n      <!-- ‰∏≠Èó¥ÔºöÊé®ËçêÁîµÂΩ±ÔºàÊúÄÂÆΩÔºâ -->\n      <div class=\"recommendation-column\">\n        <div class=\"recommendation-header\">\n          <p class=\"header-text\">Êé®ËçêÁîµÂΩ±</p>\n          <hr class=\"header-line\">\n        </div>\n        <div\n            v-for=\"movie in suggestMovies\"\n            :key=\"movie._id\"\n            class=\"movie-item\"\n            @click=\"goToMovieDetail(movie._id)\"\n        >\n          <div class=\"movie-header\">\n            <h2 class=\"movie-title\">{{ movie.title }}</h2>\n            <p class=\"movie-score\">ËØÑÂàÜ: {{ movie.star_mine }} / 10</p>\n          </div>\n          <div class=\"movie-details\">\n            <div class=\"left-column\">\n              <p class=\"movie-actors\">ÊºîÂëò: {{ movie.actor_actresses.join(', ') }}</p>\n              <p class=\"movie-tags\">Ê†áÁ≠æ: {{ movie.tags.join(', ') }}</p>\n              <p class=\"movie-languages\">ËØ≠Ë®Ä: {{ movie.languages.join(', ') }}</p>\n            </div>\n            <div class=\"right-column\">\n              <p class=\"movie-release\">‰∏äÊò†Êó∂Èó¥: {{ movie.release_time }}</p>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Âè≥‰æßÔºöÊØèÊó•Êé®Ëçê -->\n      <div class=\"side-column\">\n        <h3 class=\"column-title\">ÊØèÊó•Êé®Ëçê</h3>\n        <div\n            v-for=\"movie in dailyMovies\"\n            :key=\"movie._id\"\n            class=\"side-item\"\n            @click=\"goToMovieDetail(movie._id)\"\n        >\n          <div class=\"side-content\">\n            <span class=\"side-title\">{{ movie.title }}</span>\n          </div>\n          <span class=\"side-tags\">{{ movie.tags.join(', ') }}</span>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {\n  getSuggestedMovies,\n  searchMovies,\n  getAllMovies,\n  logout,\n  fuzzySearchMovies,\n  getHomePage\n} from \"@/services/api\";\nimport { mapActions } from \"vuex\";\n\nexport default {\n  data() {\n    return {\n      userTitle: '',\n      userName: '',\n      searchKeyword: '',\n      topMovies: [],      // Â∑¶‰æßÁÉ≠Â∫¶Ê¶úÊï∞ÊçÆ\n      suggestMovies: [],  // ‰∏≠Èó¥Êé®ËçêÁîµÂΩ±Êï∞ÊçÆ\n      dailyMovies: []     // Âè≥‰æßÊØèÊó•Êé®ËçêÊï∞ÊçÆ\n    }\n  },\n  methods: {\n    ...mapActions(['updateMovies']),\n    async logout() {\n      try {\n        await logout();\n        this.$router.push({name: 'Login'});\n      } catch (error) {\n        console.error('ÁôªÂá∫Â§±Ë¥•:', error);\n      }\n    },\n    async searchByKeyword() {\n      if (!this.searchKeyword) {\n        alert('ËØ∑ËæìÂÖ•ÊêúÁ¥¢ÂÖ≥ÈîÆËØç');\n        return;\n      }\n      try {\n        const response = await searchMovies(this.searchKeyword);\n        const movies = response.data.data;\n        this.updateMovies(movies);\n        this.$router.push({name: 'SearchResults'});\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    async fuzzySearch() {\n      if (!this.searchKeyword) {\n        alert('ËØ∑ËæìÂÖ•ÊêúÁ¥¢ÂÖ≥ÈîÆËØç');\n        return;\n      }\n      try{\n        const response=await fuzzySearchMovies(this.searchKeyword);\n        const movies = response.data.data;\n        this.updateMovies(movies);\n        this.$router.push({name: 'SearchResults'});\n      }catch(error){\n        console.error(error);\n      }\n    },\n    async fetchAllMovies() {\n      try {\n        const response = await getAllMovies();\n        const movies = response.data.data;\n        await this.updateMovies(movies);\n        this.$router.push({name: 'SearchResults'});\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    goToTagsSearch() {\n      this.$router.push({name: 'TagsSearch'});\n    },\n    async fetchHomeData() {\n      try {\n        // Ëé∑Âèñ‰∏ªÈ°µÊï∞ÊçÆÔºàÂåÖÂê´10‰∏™ÁîµÂΩ±Ôºâ\n        const response = await getHomePage();\n        if (response.data.code === 1 && response.data.data) {\n          const homeData = response.data.data;\n\n          // ÂàÜÂâ≤Êï∞ÊçÆÔºöÂâç5‰∏™ÊòØÁÉ≠Â∫¶Ê¶úÔºåÂêé5‰∏™ÊòØÊØèÊó•Êé®Ëçê\n          this.topMovies = homeData.slice(0, 5);\n          this.dailyMovies = homeData.slice(5, 10);\n        } else {\n          console.error('Ëé∑Âèñ‰∏ªÈ°µÊï∞ÊçÆÂ§±Ë¥•:', response.data.msg);\n        }\n      } catch (error) {\n        console.error('Ëé∑Âèñ‰∏ªÈ°µÊï∞ÊçÆÂ§±Ë¥•', error);\n      }\n    },\n    async fetchSuggestedMovies() {\n      try {\n        const response = await getSuggestedMovies();\n        if (respon