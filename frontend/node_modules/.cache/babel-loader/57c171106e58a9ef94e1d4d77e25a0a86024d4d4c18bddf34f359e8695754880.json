|| (_cache[15] = _createElementVNode(\"h3\", {\n    \"class\": \"column-title\"\n  }, \"‰∏™ÊÄßÊé®Ëçêüíï\", -1 /* CACHED */)), (_openBlock(true), _createElementBlock(_Fragment, null, _renderList($data.dailyMovies, function (movie) {\n    return _openBlock(), _createElementBlock(\"div\", {\n      key: movie._id,\n      \"class\": \"side-item\",\n      onClick: function onClick($event) {\n        return $options.goToMovieDetail(movie._id);\n      }\n    }, [_createElementVNode(\"div\", _hoisted_27, [_createElementVNode(\"span\", _hoisted_28, _toDisplayString(movie.title), 1 /* TEXT */)]), _createElementVNode(\"span\", _hoisted_29, _toDisplayString(movie.tags.join(', ')), 1 /* TEXT */)], 8 /* PROPS */, _hoisted_26);\n  }), 128 /* KEYED_FRAGMENT */))])])]);\n}","map":{"version":3,"names":["_createElementBlock","_hoisted_1","_createElementVNode","_hoisted_2","_toDisplayString","$data","userTitle","userName","onClick","_cache","$options","logout","apply","arguments","_hoisted_3","type","searchKeyword","$event","onKeydown","handleKeydown","placeholder","searchByKeyword","fuzzySearch","_hoisted_4","fetchAllMovies","goToTagsSearch","_createCommentVNode","_hoisted_5","_hoisted_6","_Fragment","_renderList","topMovies","movie","index","key","_id","goToMovieDetail","_hoisted_8","_hoisted_9","_hoisted_10","title","_hoisted_11","tags","join","_hoisted_12","suggestMovies","_hoisted_14","_hoisted_15","_hoisted_16","_hoisted_17","star_mine","_hoisted_18","_hoisted_19","_hoisted_20","actor_actresses","_hoisted_21","_hoisted_22","languages","_hoisted_23","_hoisted_24","release_time","_hoisted_25","dailyMovies","_hoisted_27","_hoisted_28","_hoisted_29"],"sources":["/Users/lingl/Desktop/java_proj/movie_night/MovieNight/frontend/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div class=\"user-info\">\n      <span>Enjoy your movie night, {{ userTitle+\" \"+userName+ \" üò∫\" }}</span>\n      <button @click=\"logout\" class=\"logout-button\">ÁôªÂá∫</button>\n    </div>\n    <div class=\"typing-effect\">\n      <p class=\"typing-text\">Select one for movie night</p>\n    </div>\n    <div class=\"search-container\">\n      <input\n          type=\"text\"\n          v-model=\"searchKeyword\"\n          @keydown=\"handleKeydown\"\n          placeholder=\"ËæìÂÖ•ÁîµÂΩ±Âêç / ÂØºÊºî / ÊºîÂëòÁ≤æÁ°ÆÊêúÁ¥¢Ô∏èüôÇ ÊàñËÄÖ ËæìÂÖ•ÁîµÂΩ±ÊèèËø∞Ê®°Á≥äÊêúÁ¥¢üò∂‚Äçüå´\"\n          class=\"search-box\"\n      />\n      <button @click=\"searchByKeyword\" class=\"search-button\">Á≤æÁ°ÆÊêúÁ¥¢</button>\n      <button @click=\"fuzzySearch\" class=\"fuzzy-search-button\">Ê®°Á≥äÊêúÁ¥¢</button>\n    </div>\n    <div class=\"category-search-container\">\n      <button @click=\"fetchAllMovies\" class=\"view-all-button\">Êü•ÁúãÂÖ®ÈÉ®ÁîµÂΩ±üòé</button>\n      <button @click=\"goToTagsSearch\" class=\"category-search-button\">ÈÄöËøáÊ†áÁ≠æÊêúÁ¥¢üòé</button>\n    </div>\n\n    <!-- ‰∏âÂàóÂ∏ÉÂ±ÄÂÆπÂô® -->\n    <div class=\"three-column-container\">\n      <!-- Â∑¶‰æßÔºöÁÉ≠Â∫¶Ê¶ú -->\n      <div class=\"side-column\">\n        <h3 class=\"column-title\">ÁÉ≠Â∫¶Ê¶úüî•</h3>\n        <div\n            v-for=\"(movie, index) in topMovies\"\n            :key=\"movie._id\"\n            class=\"side-item\"\n            @click=\"goToMovieDetail(movie._id)\"\n        >\n          <div class=\"side-content\">\n            <span class=\"side-index\">{{ index + 1 }}.</span>\n            <span class=\"side-title\">{{ movie.title }}</span>\n          </div>\n          <span class=\"side-tags\">{{ movie.tags.join(', ') }}</span>\n        </div>\n      </div>\n\n      <!-- ‰∏≠Èó¥ÔºöÊé®ËçêÁîµÂΩ±ÔºàÊúÄÂÆΩÔºâ -->\n      <div class=\"recommendation-column\">\n        <div class=\"recommendation-header\">\n          <p class=\"header-text\">Êé®ËçêÁîµÂΩ±</p>\n          <hr class=\"header-line\">\n        </div>\n        <div\n            v-for=\"movie in suggestMovies\"\n            :key=\"movie._id\"\n            class=\"movie-item\"\n            @click=\"goToMovieDetail(movie._id)\"\n        >\n          <div class=\"movie-header\">\n            <h2 class=\"movie-title\">{{ movie.title }}</h2>\n            <p class=\"movie-score\">ËØÑÂàÜ: <span class=\"score-value\">{{ movie.star_mine }}</span> / 10</p>\n          </div>\n          <div class=\"movie-details\">\n            <div class=\"left-column\">\n              <p class=\"movie-actors\"><span class=\"detail-label\">ÊºîÂëò:</span> {{ movie.actor_actresses.join(', ') }}</p>\n              <p class=\"movie-tags\"><span class=\"detail-label\">Ê†áÁ≠æ:</span> {{ movie.tags.join(', ') }}</p>\n              <p class=\"movie-languages\"><span class=\"detail-label\">ËØ≠Ë®Ä:</span> {{ movie.languages.join(', ') }}</p>\n            </div>\n            <div class=\"right-column\">\n              <p class=\"movie-release\"><span class=\"detail-label\">‰∏äÊò†Êó∂Èó¥:</span> {{ movie.release_time }}</p>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Âè≥‰æßÔºöÊØèÊó•Êé®Ëçê -->\n      <div class=\"side-column\">\n        <h3 class=\"column-title\">‰∏™ÊÄßÊé®Ëçêüíï</h3>\n        <div\n            v-for=\"movie in dailyMovies\"\n            :key=\"movie._id\"\n            class=\"side-item\"\n            @click=\"goToMovieDetail(movie._id)\"\n        >\n          <div class=\"side-content\">\n            <span class=\"side-title\">{{ movie.title }}</span>\n          </div>\n          <span class=\"side-tags\">{{ movie.tags.join(', ') }}</span>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {\n  getSuggestedMovies,\n  searchMovies,\n  getAllMovies,\n  logout,\n  fuzzySearchMovies,\n  getHomePage\n} from \"@/services/api\";\nimport { mapActions } from \"vuex\";\n\nexport default {\n  data() {\n    return {\n      userTitle: '',\n      userName: '',\n      searchKeyword: '',\n      topMovies: [],      // Â∑¶‰æßÁÉ≠Â∫¶Ê¶úÊï∞ÊçÆ\n      suggestMovies: [],  // ‰∏≠Èó¥Êé®ËçêÁîµÂΩ±Êï∞ÊçÆ\n      dailyMovies: []     // Âè≥‰æßÊØèÊó•Êé®ËçêÊï∞ÊçÆ\n    }\n  },\n  methods: {\n    ...mapActions(['updateMovies']),\n    async logout() {\n      try {\n        await logout();\n        this.$router.push({name: 'Login'});\n      } catch (error) {\n        console.error('ÁôªÂá∫Â§±Ë¥•:', error);\n      }\n    },\n    async searchByKeyword() {\n      if (!this.searchKeyword) {\n        alert('ËØ∑ËæìÂÖ•ÊêúÁ¥¢ÂÖ≥ÈîÆËØç');\n        return;\n      }\n      try {\n        const response = await searchMovies(this.searchKeyword);\n        const movies = response.data.data;\n        this.updateMovies(movies);\n        this.$router.push({name: 'SearchResults'});\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    async fuzzySearch() {\n      if (!this.searchKeyword) {\n        alert('ËØ∑ËæìÂÖ•ÊêúÁ¥¢ÂÖ≥ÈîÆËØç');\n        return;\n      }\n      try{\n        const response=await fuzzySearchMovies(this.searchKeyword);\n        const movies = response.data.data;\n        this.updateMovies(movies);\n        this.$router.push({name: 'SearchResults'});\n      }catch(error){\n        console.error(error);\n      }\n    },\n    async fetchAllMovies() {\n      try {\n        const response = await getAllMovies();\n        const movies = response.data.data;\n        await this.updateMovies(movies);\n        this.$router.push({name: 'SearchResults'});\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    goToTagsSearch() {\n      this.$router.push({name: 'TagsSearch'});\n    },\n    async fetchHomeData() {\n      try {\n        // Ëé∑Âèñ‰∏ªÈ°µÊï∞ÊçÆÔºàÂåÖÂê´10‰∏™ÁîµÂΩ±Ôºâ\n        const response = await getHomePage();\n        if (response.data.code === 1 && response.data.data) {\n          const homeData = response.data.data;\n\n          // ÂàÜÂâ≤Êï∞ÊçÆÔºöÂâç5‰∏™ÊòØÁÉ≠Â∫¶Ê¶úÔºåÂêé5‰∏™ÊòØÊØèÊó•Êé®Ëçê\n          this.topMovies = homeData.slice(0, 5);\n          this.dailyMovies = homeData.slice(5, 10);\n        } else {\n          console.error('Ëé∑Âèñ‰∏ªÈ°µÊï∞ÊçÆÂ§±Ë¥•:', response.data.msg);\n        }\n      } catch (error) {\n        console.error('Ëé∑Âèñ‰∏ªÈ°µÊï∞ÊçÆÂ§±Ë¥•', error);\n      }\n    },\n    async fetchSuggestedMovies() {\n      try {\n        const response = await getSuggestedMovies();\n        if (response.data.code === 1 && response.data.data) {\n          this.suggestMovies = response.data.data;\n        } else {\n          console.error('Ëé∑ÂèñÊé®ËçêÁîµÂΩ±Â§±Ë¥•:', response.data.msg);\n        }\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    goToMovieDetail(_id) {\n      this.$router.push({name: 'MovieDetail', params: {_id}});\n    },\n    handleKeydown(event) {\n      if (event.key === 'Enter') {\n        if (confirm('ËØ∑ÈÄâÊã©ÊêúÁ¥¢ÊñπÂºèÔºö\\n\\nÁ°ÆÂÆö - Á≤æÁ°ÆÊêúÁ¥¢\\nÂèñÊ∂à - Ê®°Á≥äÊêúÁ¥¢')) {\n          this.searchByKeyword();\n        } else {\n          this.fuzzySearch();\n        }\n      }\n    }\n  },\n  created() {\n    // ‰ªéÊú¨Âú∞Â≠òÂÇ®Ëé∑ÂèñÁî®Êà∑‰ø°ÊÅØ\n    const userInfo = JSON.parse(localStorage.getItem('userInfo'));\n    if (userInfo) {\n      this.userTitle = userInfo.title;\n      this.userName = userInfo.nickname;\n    }\n\n    // ÂêåÊó∂Ëé∑Âèñ‰∏âÁßçÊï∞ÊçÆ\n    this.fetchHomeData();\n    this.fetchSuggestedMovies();\n  }\n}\n</script>\n\n<style scoped>\nhtml, body {\n  margin: 0;\n  padding: 0;\n  height: 100%;\n}\n\n.user-info {\n  position: absolute;\n  top: 20px;\n  right: 20px;\n  display: flex;\n  align-items: center;\n  gap: 10px;\n}\n\n.logout-button {\n  padding: 5px 10px;\n  background-color: #b79292;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.logout-button:hover {\n  background-color: #883333;\n}\n\n\n.home {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-top: 0;\n  padding-top: 0;\n}\n\n.typing-effect {\n  text-align: center;\n  margin-top: 0;\n  padding-top: 0;\n}\n\n.typing-text {\n  font-size: 30px;\n  font-weight: bold;\n  font-family: 'Technic';\n  overflow: hidden;\n  white-space: