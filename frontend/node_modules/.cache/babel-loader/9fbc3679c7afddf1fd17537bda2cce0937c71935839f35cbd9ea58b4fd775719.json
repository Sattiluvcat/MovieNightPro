  this.fuzzySearch();\n        }\n      }\n    }\n  }),\n  created: function created() {\n    // ‰ªéÊú¨Âú∞Â≠òÂÇ®Ëé∑ÂèñÁî®Êà∑‰ø°ÊÅØ\n    console.log(\"HomeView created\");\n    var userInfo = JSON.parse(localStorage.getItem('userInfo'));\n    if (userInfo) {\n      this.userTitle = userInfo.title;\n      this.userName = userInfo.nickname;\n    }\n    this.fetchSuggestedMovies();\n    this.fetchHomeData();\n  }\n};","map":{"version":3,"names":["getSuggestedMovies","searchMovies","getAllMovies","logout","fuzzySearchMovies","getHomeData","mapActions","data","userTitle","userName","searchKeyword","movies","mostViewMovies","dailyRecommendMovies","methods","_objectSpread","_this","_asyncToGenerator","_regenerator","m","_callee","_t","w","_context","n","p","$router","push","name","v","console","error","a","fetchHomeData","_this2","_callee2","response","homeData","_t2","_context2","slice","searchByKeyword","_this3","_callee3","_t3","_context3","alert","updateMovies","fuzzySearch","_this4","_callee4","_t4","_context4","fetchAllMovies","_this5","_callee5","_t5","_context5","log","goToTagsSearch","fetchSuggestedMovies","_this6","_callee6","_t6","_context6","goToMovieDetail","_id","params","handleKeydown","event","key","confirm","created","userInfo","JSON","parse","localStorage","getItem","title","nickname"],"sources":["/Users/lingl/Desktop/java_proj/movie_night/MovieNight/frontend/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div class=\"user-info\">\n      <span>Enjoy your movie night, {{ userTitle+\" \"+userName+ \" üò∫\" }}</span>\n      <button @click=\"logout\" class=\"logout-button\">ÁôªÂá∫</button>\n    </div>\n    <div class=\"typing-effect\">\n      <p class=\"typing-text\">Select one for movie night</p>\n    </div>\n    <div class=\"search-container\">\n      <input\n          type=\"text\"\n          v-model=\"searchKeyword\"\n          @keydown=\"handleKeydown\"\n          placeholder=\"ËæìÂÖ•ÁîµÂΩ±Âêç / ÂØºÊºî / ÊºîÂëòÁ≤æÁ°ÆÊêúÁ¥¢Ô∏èüôÇ ÊàñËÄÖ ËæìÂÖ•ÁîµÂΩ±ÊèèËø∞Ê®°Á≥äÊêúÁ¥¢üò∂‚Äçüå´\"\n          class=\"search-box\"\n      />\n      <button @click=\"searchByKeyword\" class=\"search-button\">Á≤æÁ°ÆÊêúÁ¥¢</button>\n      <button @click=\"fuzzySearch\" class=\"fuzzy-search-button\">Ê®°Á≥äÊêúÁ¥¢</button>\n    </div>\n    <div class=\"category-search-container\">\n      <button @click=\"fetchAllMovies\" class=\"view-all-button\">Êü•ÁúãÂÖ®ÈÉ®ÁîµÂΩ±üòé</button>\n      <button @click=\"goToTagsSearch\" class=\"category-search-button\">ÈÄöËøáÊ†áÁ≠æÊêúÁ¥¢üòé</button>\n    </div>\n    <div class=\"recommendation-container\">\n      <!-- Â∑¶‰æßÔºöÁÉ≠Â∫¶ÊéíË°åÊ¶ú (20%ÂÆΩÂ∫¶) -->\n      <div class=\"recommendation-column left-column\">\n        <div class=\"list-header\">\n          <h3>ÁÉ≠Â∫¶ÊéíË°åÊ¶úüî•</h3>\n          <hr class=\"header-line\">\n        </div>\n        <div class=\"top-list-container\">\n          <div v-for=\"(movie, index) in mostViewMovies\" :key=\"'mostview-'+index\"\n               class=\"top-list-item\" @click=\"goToMovieDetail(movie._id)\">\n            <span class=\"rank\">{{ index + 1 }}.</span>\n            <div class=\"movie-info\">\n              <p class=\"movie-title-small\">{{ movie.title }}</p>\n              <p class=\"movie-tags-small\">Ê†áÁ≠æ: {{ movie.tags.slice(0, 2).join(', ') }}</p>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- ‰∏≠Èó¥ÔºöÊé®ËçêÁîµÂΩ± -->\n      <div class=\"recommendation-column main-recommendation\">\n        <div class=\"recommendation-header\">\n          <p class=\"header-text\">Êé®ËçêÁîµÂΩ±</p>\n          <hr class=\"header-line\">\n        </div>\n        <div class=\"movie-overview-container\">\n          <div v-for=\"movie in movies\" :key=\"movie._id\" class=\"movie-item\" @click=\"goToMovieDetail(movie._id)\">\n            <div class=\"movie-header\">\n              <h2 class=\"movie-title\">{{ movie.title }}</h2>\n              <p class=\"movie-score\">ËØÑÂàÜ: {{ movie.star_mine }} / 10</p>\n            </div>\n            <div class=\"movie-details\">\n              <div class=\"left-info\">\n                <p class=\"movie-actors\">ÊºîÂëò: {{ movie.actor_actresses.join(', ') }}</p>\n                <p class=\"movie-tags\">Ê†áÁ≠æ: {{ movie.tags.join(', ') }}</p>\n                <p class=\"movie-languages\">ËØ≠Ë®Ä: {{ movie.languages.join(', ') }}</p>\n              </div>\n              <div class=\"right-info\">\n                <p class=\"movie-release\">‰∏äÊò†Êó∂Èó¥: {{ movie.release_time }}</p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Âè≥‰æßÔºöÊØèÊó•Êé®Ëçê -->\n      <div class=\"recommendation-column right-column\">\n        <div class=\"list-header\">\n          <h3>ÊØèÊó•Êé®Ëçê‚ú®</h3>\n          <hr class=\"header-line\">\n        </div>\n        <div class=\"top-list-container\">\n          <div v-for=\"(movie, index) in dailyRecommendMovies\" :key=\"'daily-'+index\"\n               class=\"top-list-item\" @click=\"goToMovieDetail(movie._id)\">\n            <span class=\"rank\">{{ index + 1 }}.</span>\n            <div class=\"movie-info\">\n              <p class=\"movie-title-small\">{{ movie.title }}</p>\n              <p class=\"movie-tags-small\">Ê†áÁ≠æ: {{ movie.tags.slice(0, 2).join(', ') }}</p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {getSuggestedMovies, searchMovies, getAllMovies, logout, fuzzySearchMovies, getHomeData} from \"@/services/api\";\nimport { mapActions } from \"vuex\";\n\nexport default {\n  data() {\n    return {\n      userTitle: '',\n      userName: '',\n      searchKeyword: '',\n      movies: [],\n      mostViewMovies: [],   // Êñ∞Â¢ûÔºöÊéíË°åÊ¶úÊï∞ÊçÆ\n      dailyRecommendMovies: [] // Êñ∞Â¢ûÔºöÊØèÊó•Êé®ËçêÊï∞ÊçÆ\n    }\n  },\n  methods: {\n    ...mapActions(['updateMovies']),\n    async logout() {\n      try {\n        await logout();\n        this.$router.push({name: 'Login'});\n      } catch (error) {\n        console.error('ÁôªÂá∫Â§±Ë¥•:', error);\n      }\n    },\n    async fetchHomeData(){\n      try {\n        const response = await getHomeData();\n        const homeData = response.data.data;\n        // Ââç5‰∏™ÊòØÊéíË°åÊ¶úÔºåÂêé5‰∏™ÊòØÊØèÊó•Êé®Ëçê\n        this.mostViewMovies = homeData.slice(0, 5);\n        this.dailyRecommendMovies = homeData.slice(5, 10);\n      } catch (error) {\n        console.error('Ëé∑Âèñ‰∏ªÈ°µÊï∞ÊçÆÂ§±Ë¥•:', error);\n      }\n    },\n    async searchByKeyword() {\n      if (!this.searchKeyword) {\n        alert('ËØ∑ËæìÂÖ•ÊêúÁ¥¢ÂÖ≥ÈîÆËØç');\n        return;\n      }\n      try {\n        const response = await searchMovies(this.searchKeyword);\n        const movies = response.data.data;\n        this.updateMovies(movies);\n        this.$router.push({name: 'SearchResults'});\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    async fuzzySearch() {\n      if (!this.searchKeyword) {\n        alert('ËØ∑ËæìÂÖ•ÊêúÁ¥¢ÂÖ≥ÈîÆËØç');\n        return;\n      }\n      try{\n        const response=await fuzzySearchMovies(this.searchKeyword);\n        const movies = response.data.data;\n        this.updateMovies(movies);\n        this.$router.push({name: 'SearchResults'});\n      }catch(error){\n        console.error(error);\n      }\n    },\n    async fetchAllMovies() {\n      try {\n        const response = await getAllMovies();\n        console.log(response);\n        const movies = response.data.data;\n        await this.updateMovies(movies);\n        this.$router.push({name: 'SearchResults'});\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    goToTagsSearch() {\n      this.$router.push({name: 'TagsSearch'});\n    },\n    async fetchSuggestedMovies() {\n      try {\n        console.log(\"Fetching suggested movies...\");\n        const response = await getSuggestedMovies();\n        this.movies = response.data.data;\n      } catch (error) {\n        consol